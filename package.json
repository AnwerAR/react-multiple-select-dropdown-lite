{
  "name": "react-multiple-select-dropdown-lite",
  "version": "1.1.0",
  "description": "React Multiple Select Light Weight Dropdown list | Searchable | Addable | Groupable etc",
  "author": "Arif-un",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arif-un/react-multiple-select-dropdown-lite.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.jsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --compress --css-modules false",
    "start": "microbundle-crl watch --css-modules false",
    "example": "cd example && npm start",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^3.0.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "npm",
    "react",
    "dropdown",
    "multilist",
    "list",
    "autocomplete",
    "multiselect-react-dropdown",
    "react-multiselect-dropdown",
    "multiselect react dropdown",
    "react multiselect dropdown",
    "combobox",
    "form",
    "input",
    "multiselect",
    "react-component",
    "select",
    "ui"
  ],
  "bugs": {
    "url": "https://github.com/Arif-un/react-multiple-select-dropdown-lite/issues"
  },
  "homepage": "https://github.com/Arif-un/react-multiple-select-dropdown-lite#readme",
  "directories": {
    "example": "example"
  }
}
